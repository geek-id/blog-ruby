      <!--/start-sign-up-->
  <div class="sign_main">
    <h4 class="side">Sign  Up  for    Newsletter</h4>
    <div class="sign_up">
        <% flash.each do |message_type, message| %>
          <div class="alert alert-<%= message_type %>"><%= message %></div>
        <% end %>
         <p class="sign">Sign up to receive our free newsletters!</p>
           <%= form_for @subcriber, url: subcriber_index_path(@subcriber) do |f| %>
              <%= render 'shared/error_messages', object: f.object %>
              <%= f.text_field :name, placeholder: "Name", class: 'text' %>
              <%= f.email_field :email, placeholder: "Email Address", class: 'text' %>
              <%= f.submit 'Subscribe'%>
           <% end %>
       <p class="spam">We do not spam. We value your privacy!</p>
    </div>
  </div>
       <!--//end-sign-up-->
  <h4 class="side">Latest Posts</h4>
     <div class="edit-pics">

       <% @posts.each do |posts| %>
       <div class="editor-pics">
         <div class="col-md-3 item-pic">
         <% if posts.images.present? %>
            <%= image_tag posts.images, class: "img-responsive" %>
         <% else %>
            <%= image_tag 'default.png', class: "img-responsive" %>
         <% end %>
         </div>
         <div class="col-md-9 item-details">
            <h5 class="inner two"><%= link_to posts.title, content_path(posts) %></h5>
            <div class="td-post-date two">
                <i class="fa fa-calendar-minus-o"></i>
                Posted at <%= posts.created_at.to_date %>
            </div>
        </div>

        <div class="clearfix"></div>
       </div>
       <% end %>
       </div>
     <!--//edit-pics-->
   </div>

   <div id="fb-root"></div>
   <script>(function(d, s, id) {
     var js, fjs = d.getElementsByTagName(s)[0];
     if (d.getElementById(id)) return;
     js = d.createElement(s); js.id = id;
     js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.9&appId=536059693208331";
     fjs.parentNode.insertBefore(js, fjs);
   }(document, 'script', 'facebook-jssdk'));</script>
